$Script=end;
$Script=
$Script=	for (i = 1, 16) begin
$Script=		for (j = 1, 13) begin
$Script=			M[j,i] = 0.996 * H[i] * k_n[j];
$Script=		end;
$Script=	end;
$Script=
$Script=end;
