$Script=//Задана как табличная функция двух аргументов: расхода и частоты вращения
$Script=var Y[2];
$Script=Y[1] = 1;
$Script=Y[2] = 0;
$Script=
$Script=var X[11];
$Script=var deltaX;
$Script=deltaX = 100 / 3600 / 10;
$Script=var i: integer;
$Script=
$Script=for (i = 1, 11) begin
$Script=	X[i] = deltaX * (i - 1);
$Script=end;
$Script=
$Script=var a, b, c, d, e;
$Script=
$Script=a = 6.662E+08;
$Script=b = - 2.693E+07;
$Script=c = 6.509E+04;
$Script=d = 7.484E+03;
$Script=e = 2.875E-01;
$Script=
$Script=var M[2, 11];
$Script=for (i = 1, 11) begin
$Script=	M[1, i] = (a * (deltaX * (i - 1)) ^ 4 + b * (deltaX * (i - 1)) ^ 3
$Script=	+ c * (deltaX * (i - 1)) ^ 2 + d * deltaX * (i - 1) + e) * 1e-2;
$Script=	M[2, i] = 0;
$Script=	if M[1, i] < 0 then begin
$Script=		M[1, i] = 0;
$Script=	end;
$Script=end;
