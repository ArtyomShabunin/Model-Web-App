$Script=//ХАРАКТЕРИСТИКА ЭКОНОМИЧНОСТИ НАСОСА 1Д 1250-63а "Ливгидромаш", 980 об./мин
$Script=//Задана как табличная функция двух аргументов: расхода и частоты вращения
$Script=var Y[2];
$Script=Y[1] = 1;
$Script=Y[2] = 0;
$Script=
$Script=var X[11];
$Script=var deltaX;
$Script=deltaX = 1500 / 3600 / 10;
$Script=var i: integer;
$Script=
$Script=for (i = 1, 11) begin
$Script=	X[i] = deltaX * (i - 1);
$Script=end;
$Script=
$Script=var a, b, c, d;
$Script=
$Script=a = 3.521E+03;
$Script=b = - 3.265E+03;
$Script=c = 9.285E+02;
$Script=d = 5.428E-01;
$Script=
$Script=var M[2, 11];
$Script=for (i = 1, 11) begin
$Script=	M[1, i] = (a * (deltaX * (i - 1)) ^ 3 + b * (deltaX * (i - 1)) ^ 2
$Script=	+ c * deltaX * (i - 1) + d) * 1e-2;
$Script=	M[2, i] = 0;
$Script=	if M[1, i] < 0.05 then begin
$Script=		M[1, i] = 0.05;
$Script=	end;
$Script=end;
