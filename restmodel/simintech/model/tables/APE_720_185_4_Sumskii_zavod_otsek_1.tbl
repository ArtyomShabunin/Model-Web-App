$Script=for (i2 = 1, 11) begin
$Script=	for (i1 = 1, 7) begin
$Script=		M[i1, i2] = (a[i1] * (deltaX * (i2 - 1)) ^ 3 + b[i1] * (deltaX * (i2 - 1)) ^ 2
$Script=		+ c[i1] * deltaX * (i2 - 1) + d[i1]) * 1e-2 * 0.57;
$Script=		if M[i1, i2] < 0 then begin
$Script=			M[i1, i2] = 0;
$Script=		end;
$Script=	end;
$Script=end;
