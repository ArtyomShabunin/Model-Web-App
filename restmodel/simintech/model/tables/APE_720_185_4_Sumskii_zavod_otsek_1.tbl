$Script=//НАПОРНО-РАСХОДНАЯ ХАРАКТЕРИСТИКА НАСОСА АПЭ 720-185-4, промотбор
$Script=//Задана как табличная функция двух аргументов: расхода и частоты вращения
$Script=
$Script=var Y[2];
$Script=Y[1] = 1;
$Script=Y[2] = 0;
$Script=
$Script=var X[11];
$Script=var deltaX;
$Script=deltaX = 1500 / 3600 / 10;
$Script=var i: integer, i1: integer, i2: integer;
$Script=
$Script=for (i = 1, 11) begin
$Script=	X[i] = deltaX * (i - 1)
$Script=end;
$Script=
$Script=var a[2], b[2], c[2];
$Script=a[1] = - 3.170E+03;
$Script=a[2] = 0;
$Script=b[1] = 3.331E+02;
$Script=b[2] = 0;
$Script=c[1] = 6.781E+02;
$Script=c[2] = 0;
$Script=
$Script=var M[2, 11];
$Script=for (i2 = 1, 11) begin
$Script=	for (i1 = 1, 2) begin
$Script=		M[i1, i2] = a[i1] * (deltaX * (i2 - 1))^2 + b[i1] * (deltaX * (i2 - 1)) + c[i1];
$Script=	end
$Script=end;
$Script=
